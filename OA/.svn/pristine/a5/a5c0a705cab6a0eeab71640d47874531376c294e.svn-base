<?xml version="1.0" encoding="UTF-8"?>
<taglib>
    <tlibversion>1.0</tlibversion>
    <jspversion>1.1</jspversion>
    <shortname>bean</shortname>
    <uri>com.joinsoft.masterpage</uri>
    <tag>
        <name>Content</name>
        <tagclass>cn.tempus.tags.ContentTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>contentPlaceHolderId</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>ContentPlaceHolder</name>
        <tagclass>cn.tempus.tags.ContentPlaceHolderTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>id</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>MasterPage</name>
        <tagclass>cn.tempus.tags.MasterPageTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>id</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>ContentPage</name>
        <tagclass>cn.tempus.tags.ContentPageTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>materPageId</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
</taglib>